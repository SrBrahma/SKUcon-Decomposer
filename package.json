{
  "name": "skucon-decomposer",
  "version": "1.0.0",
  "description": "A simple **npm package** to decompose Android/iOS opinionated SKUs in my convetion/pattern, to allow information changes and additions to the SKUs easily and future-proofable.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -d",
    "deploy": "npm run build && npm publish",
    "major": "npm --no-git-tag-version version major",
    "minor": "npm --no-git-tag-version version minor",
    "minorD": "npm run minor && npm run deploy",
    "patch": "npm --no-git-tag-version version patch",
    "patchD": "npm run patch && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SrBrahma/SKUcon-Decomposer.git"
  },
  "keywords": [
    "npm",
    "sku",
    "package",
    "naming",
    "pattern"
  ],
  "author": "SrBrahma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SrBrahma/SKUcon-Decomposer/issues"
  },
  "homepage": "https://github.com/SrBrahma/SKUcon-Decomposer#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist"
  ]
}